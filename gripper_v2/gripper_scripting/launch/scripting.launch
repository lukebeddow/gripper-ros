<launch>

	<!-- run the auto yaml script to add finger joints to the controllers -->
  <node name="auto_yaml" pkg="gripper_scripting" type="generate_controller_yaml.py" output="screen"
  	respawn="false"/>

	<!-- create a node to parse panda.srdf using xacro, and save a new file called test_panda.srdf -->
  <node name="xacro" pkg="xacro" type="xacro" respawn="false" required="true"
  			args="--inorder -o $(find gripper_moveit)/config/robot_model_autogenerated.srdf $(find gripper_moveit)/config/robot_model.xacro"/>
  			
  <!-- the second node is 'required' and so roslaunch will exit once that node terminates. This node is chosen to
  		 trigger shutdown since it takes longer -->
  			
</launch>
